        <h1 class="sect-header">Librarians manager</h1>
        <div id="lower-container">
            
            <div id="librarians-container">
                <h1 class="lib-header">Registered Librarians</h1>
                <ul class="people-container">

                    <li class="information-container">
                        <div class="information-left">
                            <img class="rounded-full overflow-hidden w-20 h-20 p-3" src="/images/lib1.jpg" alt="">
                            <div class="name-mail-container">
                            <p class="name-librarian">Name: </p>
                            <p class="email-librarian">Email: </p>
                            </div>
                        </div>
                        <form action="" method="POST">
                            <button type="submit">Delete</button>
                        </form>
                    </li>
                    <li class="information-container">
                        <div class="information-left">
                            <img class="rounded-full overflow-hidden w-20 h-20 p-3" src="/images/lib1.jpg" alt="">
                            <div class="name-mail-container">
                            <p class="name-librarian">Name: </p>
                            <p class="email-librarian">Email: </p>
                            </div>
                        </div>
                        <form action="" method="POST">
                            <button type="submit">Delete</button>
                        </form>
                    </li>
                    
                    <% librarians.forEach(function(librarian) { %>
                        <li>
                            <p>Name: <%= librarian.username %></p>
                            <p>Email: <%= librarian.gmail %></p>
                            <p>Profile Picture: <img src="/<%= librarian.profilePicture %>" alt="<%= librarian.username %>'s Profile picture"></p>
                            <form action="librarian/<%= librarian._id %>" method="POST">
                                <button type="submit">Delete</button>
                            </form>
                        </li>
                    <% }); %>
                </ul>
            </div>
            
            <div id="verifying-container">
                <h1 class="lib-header">Verifying Librarians</h1>
                <ul class="people-container">
                    <li class="information-container">
                        <div class="information-left">
                            <img class="rounded-full overflow-hidden w-20 h-20 p-3" src="/images/lib1.jpg" alt="">
                            <div class="name-mail-container">
                            <p class="name-librarian">Name: </p>
                            <p class="email-librarian">Email: </p>
                            </div>
                        </div>
                        <form action="" method="POST">
                            <button type="submit">Delete</button>
                        </form>
                    </li>
                    <li class="information-container">
                        <div class="information-left">
                            <img class="rounded-full overflow-hidden w-20 h-20 p-3" src="/images/lib1.jpg" alt="">
                            <div class="name-mail-container">
                            <p class="name-librarian">Name: </p>
                            <p class="email-librarian">Email: </p>
                            </div>
                        </div>
                        <form action="" method="POST">
                            <button type="submit">Delete</button>
                        </form>
                    </li>
                    <% verifyingLibrarians.forEach(function(librarian) { %>
                        <li>
                            <p>Name: <%= librarian.username %></p>
                            <p>Email: <%= librarian.gmail %></p>
                            <form action="verifying_librarian/<%= librarian._id %>" method="POST">
                                <button type="submit">Delete</button>
                            </form>
                        </li>
                    <% }); %>
                </ul>  
            </div>
        </div>
        
        
